generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Vessel {
  id      Int      @id @default(autoincrement())
  name    String
  type    String
  tonnage Int
  length  Int
  breadth Int
  eta     DateTime
}

model User {
  id        Int     @id @default(autoincrement())
  email     String
  password  String
  name      String? 
}